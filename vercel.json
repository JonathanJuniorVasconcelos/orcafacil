{
  "projects": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "painel/package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "painel/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "^/painel(?:/.*)?$",
      "dest": "painel/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}